<Window x:Class="StartLauncher.LaunchObjectsPickers.UrlPickerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StartLauncher.LaunchObjectsPickers"
        mc:Ignorable="d"
        Title="Url Picker" Height="80" Width="500" ResizeMode="NoResize">
    <StackPanel Orientation="Horizontal" Margin="5">
        <Label Content="URL:" Margin="0,0,5,0" VerticalAlignment="Center"/>
        <TextBox x:Name="UrlText" Margin="0,0,5,0" VerticalAlignment="Center" HorizontalAlignment="Stretch" Height="30" Width="400" VerticalContentAlignment="Center"/>
        <Button x:Name="ConfirmUrl" Margin="0,0,5,0" Content="OK" Click="ConfirmUrl_Click" VerticalAlignment="Center" Height="30" IsDefault="True"/>
    </StackPanel>
</Window>
