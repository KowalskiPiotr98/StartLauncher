<Window x:Class="Start_Launcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Start_Launcher"
        mc:Ignorable="d"
        Title="Start Launcher" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Menu x:Name="TopMenu" Margin="5">
            <MenuItem Header="Options">
                <MenuItem Header="Launch on startup" Name="LaunchOnStartup" IsCheckable="True" IsChecked="True" Click="LaunchOnStartup_Click"/>
            </MenuItem>
        </Menu>
    </Grid>
</Window>
